        <item name="android:focusable">true</item>
        <item name="android:soundEffectsEnabled">false</item>
        <item name="android:stateListAnimator">@animator/keyboard_scale_anim</item>
        <item name="android:background">@drawable/rectangle_keyboard_key</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textColor">?attr/colorSecondary</item>
    </style>

    <style name="textView_keyboard_numbers_2">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:stateListAnimator">@animator/keyboard_scale_anim</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textColor">?attr/colorSecondary</item>
    </style>

    <style name="textView_keyboard_numbers_save">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
