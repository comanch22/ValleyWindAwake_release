        <item name="android:gravity">center</item>
        <item name="android:stateListAnimator">@animator/keyboard_scale_anim</item>
        <item name="android:background">@drawable/rectangle</item>
        <item name="android:textColor">?attr/colorSecondary</item>
    </style>

    <style name="textView_keyboard_numbers_land_2">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:stateListAnimator">@animator/keyboard_scale_anim</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:textColor">?attr/colorSecondary</item>
    </style>

    <style name="Switch_th" >
        <item name="colorControlActivated">?attr/colorPrimaryVariant</item>
        <item name="colorSwitchThumbNormal">?attr/colorSecondary</item>
    </style>

    <style name="textView_ringtone_numbers">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:soundEffectsEnabled">false</item>
        <item name="android:background">?attr/colorPrimaryVariant</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textColor">?attr/colorSecondary</item>
    </style>

    <style name="off_signal_button_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="android:background">?attr/colorPrimaryVariant</item>
    </style>

    <style name="front_list_fab_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="borderWidth">0dp</item>
        <item name="android:focusable">true</item>
        <item name="android:backgroundTint">?attr/colorPrimary</item>
        <item name="android:soundEffectsEnabled">false</item>
        <item name="android:textColor">?attr/colorSecondary</item>
        <item name="android:layout_gravity">start|bottom</item>
